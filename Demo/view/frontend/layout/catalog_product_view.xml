<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceContainer name="product.info.main">
                <referenceBlock name="product.info">
                    <referenceContainer name="product.info.form.content">
                        <block name="delivery-field" class="Cate\Demo\Block\Delivery" template="Cate_Demo::Delivery.phtml" before="product.info.addtocart"/>
                    </referenceContainer>
                </referenceBlock>
            </referenceContainer>
            <container name="custom-form-magenest" after="content.aside">
                <block name="custom-form-magenest-block" class="Cate\Demo\Block\KnockoutTest" template="Cate_Demo::KnockoutTest.phtml">
                </block>
            </container>
        </referenceContainer>
    </body>
</page>
